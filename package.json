{
  "name": "digicook",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "15.3.0",
    "npm": "7.0.14"
  },
  "scripts": {
    "start": "node server/dist/index.js",
    "server": "ts-node-dev server/src/index.ts",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "postinstall": "(cd server && npm install); (cd client && npm install)",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install && npm run build --prefix server && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mhyusuf/digicook.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mhyusuf/digicook/issues"
  },
  "homepage": "https://github.com/mhyusuf/digicook#readme",
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.7",
    "babel-eslint": "^10.1.0",
    "concurrently": "^5.3.0",
    "cypress": "^5.6.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.13.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "nodemon": "^2.0.6",
    "prettier": "^2.1.2",
    "ts-node-dev": "^1.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "eslint-plugin-jest-dom": "^3.2.4",
    "eslint-plugin-testing-library": "^3.10.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ],
    "plugins": []
  }
}
